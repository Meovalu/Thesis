<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="MainSuite">
	<listeners>
		<listener class-name="com.cuong.listeners.TestListener"></listener>
	</listeners>
	<test name="Test_edge">
		<parameter name="browser" value="edge"/>
		<classes>
			<class name="com.cuong.testcases.KeywordsTest"/>
		</classes>
	</test>
	<test name="Test_chrome">
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="com.cuong.testcases.KeywordsTest"/>
		</classes>
	</test>
</suite>